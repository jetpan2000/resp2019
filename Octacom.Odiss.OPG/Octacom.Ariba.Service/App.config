    <add key="serilog:using:RollingFile" value="Serilog.Sinks.RollingFile" />
    <add key="serilog:write-to:RollingFile.pathFormat" value="E:\bak\OPG_SAMPLE_DATA\OPG.Web_log{Date}.log" />
